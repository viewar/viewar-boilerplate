{
  "name": "viewar-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate app for ViewAR SDK",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "viewar-cli deploy",
    "build": "webpack --mode production --env prod --config ./config/webpack.config.js",
    "start": "webpack-dev-server --mode development --env dev-core --open --config ./config/webpack.config.js",
    "start:core": "npm run start",
    "start:mock": "webpack-dev-server --mode development --env dev-mock --open --config ./config/webpack.config.js",
    "start:remote": "npm run start:mock"
  },
  "author": "Ivan Šakić <is@viewar.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "fastclick": "^1.0.6",
    "viewar-api": "^0.10.51"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "ip": "^1.1.5",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10",
    "webpack-concat-plugin": "^3.0.0-beta.0",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.1"
  }
}
